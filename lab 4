import java.util.Timer;
import java.util.TimerTask;
public class PrinterModel {
 public void schedulePrinting() {
 Timer timer = new Timer();
 timer.schedule(new PrintStepTimer(), 0);
 }
}
class PrintStepTimer extends TimerTask {
 @Override
 public void run() {
 System.out.println("[Printing started ...]");
 
 try {
 Thread.sleep(5000); 
 } catch (InterruptedException e) {
 throw new RuntimeException(e);
 }
 Timer postProcessingTimer = new Timer();
 postProcessingTimer.schedule(new PostProcessingStepTimer(), 0);
 }
}
class PostProcessingStepTimer extends TimerTask {
 @Override
 public void run() {
 System.out.println("[Post-processing started ...]");
 
 try {
 Thread.sleep(3000); 
 } catch (InterruptedException e) {
 throw new RuntimeException(e);
 }
 System.out.println("[Post-processing finished]");
 }
}
public class Main {
 public static void main(String[] args) {
 PrinterModel printerModel = new PrinterModel();
 printerModel.schedulePrinting();
 }
}
